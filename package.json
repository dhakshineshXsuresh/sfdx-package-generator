{
	"name": "sfdx-package-xml-generator-by-dk",
	"displayName": "Salesforce Package.xml Generator Extension for VS Code by dk",
	"description": "Extension for generating Package.xml on the Salesforce Platform by Dhakshinesh(one upgrade)",
	"version": "2.1.0",
	"publisher": "Dhakshinesh",
	"repository": "https://github.com/dhakshineshXsuresh/sfdx-package-generator",
	"icon": "icon.png",
	"keywords": [
		"sfdx",
		"salesforcedx",
		"sfdx package xml",
		"salesforce",
		"salesforce package xml generator"
	],
	"engines": {
		"vscode": "^1.76.0"
	},
	"categories": [
		"Extension Packs",
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sfdxPackageGen.chooseMetadataDK",
				"title": "Choose Metadata Components",
				"category": "DK's SFDX Package.xml Generator"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"clipboardy": "^4.0.0",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@types/clipboardy": "^1.1.0",
		"@types/node": "^20",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"typescript": "^5.7.2"
	}
}
